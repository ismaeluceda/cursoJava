package main;

import entidades.Persona;

public class MainPOO {

	public static void main(String[] args) {
		Persona p1 = new Persona();
		p1.nombre = "Ismael";
		p1.edad = 44;
		p1.peso = 78;
		
		Persona p2 = new Persona();
		p2.nombre = "Rocio";
		p2.edad = 43;
		p2.peso = 55;
		
		p1.andar(10);
		p1.metros = 45;
		
	}

}
