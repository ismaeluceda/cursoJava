package ejemplo2;

public class Espada {

	private String tipo;
	
	
	public Espada() {
		// TODO Auto-generated constructor stub
	}


	public String getTipo() {
		return tipo;
	}


	public void setTipo(String tipo) {
		this.tipo = tipo;
	}

	public void usar() {
		System.out.println("Soy una espada de tipo " + tipo);
	}
}
